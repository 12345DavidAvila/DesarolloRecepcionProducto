<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DesarolloRecepcionProducto.Vistas.BuscarProductoPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Consumir Servicios Web" FontSize="20" HorizontalTextAlignment="Center"/>
            <Label x:Name="texto" Text=""/>
            <Button x:Name="btnGet" VerticalOptions="Center" Text="Consultar" Clicked="btnGet_Clicked" BackgroundColor="Blue" TextColor="White"/>
            <ListView x:Name="Listas">
            <ListView.ItemTemplate>
            <DataTemplate>
            <ViewCell>
        <StackLayout>
                <Label x:Name="lblNombre" Text="{Binding nombre}"/>
                <Label x:Name="lblUsuario" Text="{Binding usuario}"/>
            </StackLayout>
            </ViewCell>
            </DataTemplate>
            </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>